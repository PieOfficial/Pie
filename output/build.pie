@const[
    the_answer = 12
]

@add[x,y]
    return($x + $y)

@mul[x,y]
    if($y is 1 or $y is 0)
        return($x)
    endif()
    if($y is 0)
        return(0)
    endif()

    return(call(mul,$x,$y - 1) + $x)

